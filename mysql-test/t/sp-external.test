SELECT $$Hello world!$$;
--error ER_PARSE_ERROR
SELECT $hi$Hello world!$hi$;

--echo # Check that dollars can still be used in identifiers
SELECT 1 AS $hi;
SELECT 1 AS h$i;
SELECT 1 AS h$$i;
SELECT 1 AS h$i$;

--echo # Identifiers matching dollar quotes can no longer be used
--error ER_PARSE_ERROR
SELECT 1 AS $$;
--error ER_PARSE_ERROR
SELECT 1 AS $hi$;
--error ER_PARSE_ERROR
SELECT 1 AS $h$i;

--echo # ... unless quoted
SELECT 1 AS `$$`;
SELECT 1 AS `$hi$`;
SELECT 1 AS `$h$i`;


